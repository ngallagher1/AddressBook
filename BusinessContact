import java.io.*;
import java.util.*;
import java.lang.*;
import java.text.*;

public class BusinessContact<E> extends GeneralContact<E>
{
	private String department;
	private String relationToMe;

	public BusinessContact()
	{
		super();
		department = "";
		relationToMe = "";
	}

	public BusinessContact(String first, String last, int phone, String a, String d, String relation)
	{
		super(first, last, phone, a);
		department = d;
		relationToMe = relation;
	}

}
